{
  "name": "quiz-bank",
  "author": "David",
  "productName": "BTree Quiz Portal",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "electron-is-dev": "^1.1.0",
    "electron-json-storage": "^4.1.8",
    "electron-packager": "^14.2.1",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-ace": "^8.1.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-simplemde-editor": "^4.1.0",
    "update-electron-app": "^1.5.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . quiz-bank --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BTree Quiz Portal\"",
    "package-linux": "electron-packager . quiz-bank --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon.ico --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "^5.1.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "wait-on": "^4.0.0"
  }
}
